{
  "manifest_version": 3,
  "name": "Chain-Converters",
  "version": "1.0",
  "description": "A chrome extension that converts blockchain numbers and timestamps",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["contextMenus", "storage", "activeTab"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "256": "Logo256.png"
  }
}