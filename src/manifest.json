{
  "manifest_version": 3,
  "name": "Wei to Eth",
  "version": "1.0",
  "description": "a converter of Wei to Eth",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "storage",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}